class Solve {
    int[] findTwoElement(int arr[], int n) {
        int [] arr2=new int[2];
        int [] countArray=new int[n+1];
        for(int i=0;i<n;i++){
            countArray[arr[i]]++;
        }
        for(int i=0;i<countArray.length;i++){
            if(countArray[i]==2){
                arr2[0]=i;
            }
            else if(countArray[i]==0){
                arr2[1]=i;
            }
        }
        return arr2;
    }
}
