 import java.io.*;
 import java.util.*;
 public class Main{
     public static void main(String[] args){
         Scanner sc=new Scanner(System.in);
         int T=sc.nextInt();
         while(T-->0){
             long N=sc.nextInt();
             long count=0;
             while(N>=5){
                 N=N/5;
                 count=count+N;
             }
             System.out.println(count);
         }
     }
 }


