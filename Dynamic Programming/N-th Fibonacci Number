import java.util.* ;
import java.io.*; 
public class Solution{
    public static int fibonacciNumber(int n){
        if(n==0) return 0;
        if(n==1 || n==2) return 1;
        int prev1=1;
        int prev2=1;
        for(int i=3;i<=n;i++){
        int current=(prev1+prev2)%1000000007;
        prev2=prev1;
        prev1=current;  
        }     
            return prev1%1000000007;
    }
}
