import java.io.*;
import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int T=sc.nextInt();
        int [] dp=new int[100001];
        dp[1]=2;
        dp[2]=3;
        for(int i=3;i<=100000;i++){
            dp[i]=(dp[i-1]+dp[i-2])%1000000007;
        }
        while(T-->0){
            int N=sc.nextInt();
            System.out.println(dp[N]);
        }
    }
}
