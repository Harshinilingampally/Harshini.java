import java.util.* ;
import java.io.*; 
import java.util.*;
public class Solution {
	public static int maximumNonAdjacentSum(ArrayList<Integer> nums) {
		int [] dp=new int[nums.size()];
		dp[0]=nums.get(0);
		for(int i=1;i<nums.size();i++){
			int pick=nums.get(i);
			if(i>1)
			pick+=dp[i-2];
			int notpick=0+dp[i-1];
			dp[i]=Math.max(pick,notpick);
		}
		return dp[nums.size()-1];
	}
}
