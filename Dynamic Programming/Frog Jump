import java.util.* ;
import java.io.*; 
public class Solution {   
     public static int frogJump(int n, int heights[]) {
    int previous1=0;
    int previous2=0;
        for(int i=1;i<=n-1;i++){
            int secondJump=Integer.MAX_VALUE;
        int firstJump=previous1+Math.abs(heights[i]-heights[i-1]);
        if(i>1){
         secondJump=previous2+Math.abs(heights[i]-heights[i-2]);
        }
        int current=Math.min(firstJump,secondJump);
         previous2=previous1;
         previous1=current;
        }
        return previous1;

    }
}
